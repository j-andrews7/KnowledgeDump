@article{wangCellCycleRestricts2017,
  title = {The Cell Cycle Restricts Activation-Induced Cytidine Deaminase Activity to Early {{G1}}},
  author = {Wang, Qiao and Kieffer-Kwon, Kyong-Rim and Oliveira, Thiago Y. and Mayer, Christian T. and Yao, Kaihui and Pai, Joy and Cao, Zhen and Dose, Marei and Casellas, Rafael and Jankovic, Mila and Nussenzweig, Michel C. and Robbiani, Davide F.},
  date = {2017-01-01},
  journaltitle = {Journal of Experimental Medicine},
