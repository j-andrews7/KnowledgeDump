@article{hoppEpigeneticHeterogeneityBCell2015,
  title = {Epigenetic {{Heterogeneity}} of {{B-Cell Lymphoma}}: {{Chromatin Modifiers}}},
  shorttitle = {Epigenetic {{Heterogeneity}} of {{B-Cell Lymphoma}}},
  author = {Hopp, Lydia and Nersisyan, Lilit and Löffler-Wirth, Henry and Arakelyan, Arsen and Binder, Hans},
  date = {2015-10-21},
  journaltitle = {Genes},
