  author = {Hashimoto, Ari and Takeda, Kiyoshi and Inaba, Muneo and Sekimata, Masayuki and Kaisho, Tsuneyasu and Ikehara, Susumu and Homma, Yoshimi and Akira, Shizuo and Kurosaki, Tomohiro},
  date = {2000-08-15},
  journaltitle = {J Immunol},
  volume = {165},
