@article{beguelinEZH2BCL6Cooperate2016,
  title = {{{EZH2}} and {{BCL6 Cooperate}} to {{Assemble CBX8-BCOR Complex}} to {{Repress Bivalent Promoters}}, {{Mediate Germinal Center Formation}} and {{Lymphomagenesis}}},
  author = {Béguelin, Wendy and Teater, Matt and Gearhart, Micah D. and Calvo Fernández, María Teresa and Goldstein, Rebecca L. and Cárdenas, Mariano G. and Hatzi, Katerina and Rosen, Monica and Shen, Hao and Corcoran, Connie M. and Hamline, Michelle Y. and Gascoyne, Randy D. and Levine, Ross L. and Abdel-Wahab, Omar and Licht, Jonathan D. and Shaknovich, Rita and Elemento, Olivier and Bardwell, Vivian J. and Melnick, Ari M.},
