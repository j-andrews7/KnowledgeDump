@article{brazaoLongNoncodingRNAs2016,
  title = {Long Noncoding {{RNAs}} in {{B-cell}} Development and Activation},
  author = {Brazão, Tiago F. and Johnson, Jethro S. and Müller, Jennifer and Heger, Andreas and Ponting, Chris P. and Tybulewicz, Victor L. J.},
  date = {2016-08-18},
  journaltitle = {Blood},
  volume = {128},
  number = {7},
  eprint = {27381906},
  eprinttype = {pmid},
  pages = {e10-e19},
  issn = {0006-4971},
  doi = {10.1182/blood-2015-11-680843},
  url = {http://www.ncbi.nlm.nih.gov/pmc/articles/PMC5000579/},
  urldate = {2017-02-16},
  abstract = {A total of 4516 lncRNAs were identified across multiple stages of B-cell development and activation.                                 , Long noncoding RNAs (lncRNAs) are potentially important regulators of cell differentiation and development, but little is known about their roles in B lymphocytes. Using RNA-seq and de novo transcript assembly, we identified 4516 lncRNAs expressed in 11 stages of B-cell development and activation. Most of these lncRNAs have not been previously detected, even in the closely related T-cell lineage. Comparison with lncRNAs previously described in human B cells identified 185 mouse lncRNAs that have human orthologs. Using chromatin immunoprecipitation-seq, we classified 20\% of the lncRNAs as either enhancer-associated (eRNA) or promoter-associated RNAs. We identified 126 eRNAs whose expression closely correlated with the nearest coding gene, thereby indicating the likely location of numerous enhancers active in the B-cell lineage. Furthermore, using this catalog of newly discovered lncRNAs, we show that PAX5, a transcription factor required to specify the B-cell lineage, bound to and regulated the expression of 109 lncRNAs in pro-B and mature B cells and 184 lncRNAs in acute lymphoblastic leukemia.},
  pmcid = {PMC5000579}
}

