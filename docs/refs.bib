@article{williamsComprehensiveGenomicStudy2023,
  title = {A Comprehensive Genomic Study of 390 {{H3F3A-mutant}} Pediatric and Adult Diffuse High-Grade Gliomas, {{CNS WHO}} Grade 4},
  author = {Williams, Erik A. and Brastianos, Priscilla K. and Wakimoto, Hiroaki and Zolal, Amir and Filbin, Mariella G. and Cahill, Daniel P. and Santagata, Sandro and Juratli, Tareq A.},
  date = {2023-07-31},
