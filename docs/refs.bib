@article{labiLossBH3onlyProtein2008,
  title = {Loss of the {{BH3-only}} Protein {{Bmf}} Impairs {{B}} Cell Homeostasis and Accelerates γ Irradiation–Induced Thymic Lymphoma Development},
  author = {Labi, Verena and Erlacher, Miriam and Kiessling, Stephan and Manzl, Claudia and Frenzel, Anna and O'Reilly, Lorraine and Strasser, Andreas and Villunger, Andreas},
  date = {2008-03-17},
  journaltitle = {Journal of Experimental Medicine},
  volume = {205},
  number = {3},
