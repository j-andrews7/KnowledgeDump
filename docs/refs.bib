@article{hodgesDominantnegativeSMARCA4Missense2018,
  title = {Dominant-Negative {{SMARCA4}} Missense Mutations Alter the Accessibility Landscape of Tissue-Unrestricted Enhancers},
  author = {Hodges, H. Courtney and Stanton, Benjamin Z. and Cermakova, Katerina and Chang, Chiung-Ying and Miller, Erik L. and Kirkland, Jacob G. and Ku, Wai Lim and Veverka, Vaclav and Zhao, Keji and Crabtree, Gerald R.},
  date = {2018-01},
  journaltitle = {Nat Struct Mol Biol},
