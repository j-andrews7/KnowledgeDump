  pages = {1236--1242},
  issn = {1546-1718},
  doi = {10.1038/ng.2443},
  url = {https://www.nature.com/articles/ng.2443},
  urldate = {2018-05-14},
  abstract = {We have extensively characterized the DNA methylomes of 139 patients with chronic lymphocytic leukemia (CLL) with mutated or unmutated IGHV and of several mature B-cell subpopulations through the use of whole-genome bisulfite sequencing and high-density microarrays. The two molecular subtypes of CLL have differing DNA methylomes that seem to represent epigenetic imprints from distinct normal B-cell subpopulations. DNA hypomethylation in the gene body, targeting mostly enhancer sites, was the most frequent difference between naive and memory B cells and between the two molecular subtypes of CLL and normal B cells. Although DNA methylation and gene expression were poorly correlated, we identified gene-body CpG dinucleotides whose methylation was positively or negatively associated with expression. We have also recognized a DNA methylation signature that distinguishes new clinico-biological subtypes of CLL. We propose an epigenomic scenario in which differential methylation in the gene body may have functional and clinical implications in leukemogenesis.},
  langid = {english},
  file = {C:\Users\jandrews\Zotero\storage\6FP4L4E9\Kulis et al. - 2012 - Epigenomic analysis detects widespread gene-body D.pdf}
}

@article{kulisWholegenomeFingerprintDNA2015,
  title = {Whole-Genome Fingerprint of the {{DNA}} Methylome during Human {{B}} Cell Differentiation},
  author = {Kulis, Marta and Merkel, Angelika and Heath, Simon and Queirós, Ana C. and Schuyler, Ronald P. and Castellano, Giancarlo and Beekman, Renée and Raineri, Emanuele and Esteve, Anna and Clot, Guillem and Verdaguer-Dot, Néria and Duran-Ferrer, Martí and Russiñol, Nuria and Vilarrasa-Blasi, Roser and Ecker, Simone and Pancaldi, Vera and Rico, Daniel and Agueda, Lidia and Blanc, Julie and Richardson, David and Clarke, Laura and Datta, Avik and Pascual, Marien and Agirre, Xabier and Prosper, Felipe and Alignani, Diego and Paiva, Bruno and Caron, Gersende and Fest, Thierry and Muench, Marcus O. and Fomin, Marina E. and Lee, Seung-Tae and Wiemels, Joseph L. and Valencia, Alfonso and Gut, Marta and Flicek, Paul and Stunnenberg, Hendrik G. and Siebert, Reiner and Küppers, Ralf and Gut, Ivo G. and Campo, Elías and Martín-Subero, José I.},
