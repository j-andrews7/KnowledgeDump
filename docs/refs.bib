}

@article{hjelleIncidenceHairyCell1991,
  title = {Incidence of {{Hairy Cell Leukemia}}, {{Mycosis Fungoides}}, and {{Chronic Lymphocytic Leukemia}} in {{First Known HTLV-II-Endemic Population}}},
