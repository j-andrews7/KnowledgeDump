  title = {Translocations at 8q24 Juxtapose {{MYC}} with Genes That Harbor Superenhancers Resulting in Overexpression and Poor Prognosis in Myeloma Patients},
  author = {Walker, B. A. and Wardell, C. P. and Brioli, A. and Boyle, E. and Kaiser, M. F. and Begum, D. B. and Dahir, N. B. and Johnson, D. C. and Ross, F. M. and Davies, F. E. and Morgan, G. J.},
  date = {2014-03-14},
