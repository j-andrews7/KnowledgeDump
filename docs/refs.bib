@article{gaoPCGFHomologsCBX2012,
  title = {{{PCGF Homologs}}, {{CBX Proteins}}, and {{RYBP Define Functionally Distinct PRC1 Family Complexes}}},
  author = {Gao, Zhonghua and Zhang, Jin and Bonasio, Roberto and Strino, Francesco and Sawai, Ayana and Parisi, Fabio and Kluger, Yuval and Reinberg, Danny},
