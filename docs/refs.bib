@article{marshDissectionArtifactualConfounding2022,
  title = {Dissection of Artifactual and Confounding Glial Signatures by Single-Cell Sequencing of Mouse and Human Brain},
  author = {Marsh, Samuel E. and Walker, Alec J. and Kamath, Tushar and Dissing-Olesen, Lasse and Hammond, Timothy R. and de Soysa, T. Yvanka and Young, Adam M. H. and Murphy, Sarah and Abdulraouf, Abdulraouf and Nadaf, Naeem and Dufort, Connor and Walker, Alicia C. and Lucca, Liliana E. and Kozareva, Velina and Vanderburg, Charles and Hong, Soyon and Bulstrode, Harry and Hutchinson, Peter J. and Gaffney, Daniel J. and Hafler, David A. and Franklin, Robin J. M. and Macosko, Evan Z. and Stevens, Beth},
  options = {useprefix=true},
