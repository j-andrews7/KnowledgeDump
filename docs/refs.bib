  author = {Vento-Tormo, Roser and Rodríguez-Ubreva, Javier and Lisio, Lorena Di and Islam, Abul B. M. M. K. and Urquiza, Jose M. and Hernando, Henar and López-Bigas, Nuria and Shannon-Lowe, Claire and Martínez, Nerea and Montes-Moreno, Santiago and Piris, Miguel A. and Ballestar, Esteban},
  date = {2014-09-29},
