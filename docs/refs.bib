  file = {C\:\\Users\\jandrews\\Zotero\\storage\\ZRIJAJAN\\Nagano et al. - 2013 - Single-cell Hi-C reveals cell-to-cell variability .pdf;C\:\\Users\\jandrews\\Zotero\\storage\\NPTMWE7D\\nature12593.html}
}

