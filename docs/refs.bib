@article{silveiraH3K27MDepletion2019,
  title = {H3.3 {{K27M}} Depletion Increases Differentiation and Extends Latency of Diffuse Intrinsic Pontine Glioma Growth in Vivo},
  author = {Silveira, André B. and Kasper, Lawryn H. and Fan, Yiping and Jin, Hongjian and Wu, Gang and Shaw, Timothy I. and Zhu, Xiaoyan and Larson, Jon D. and Easton, John and Shao, Ying and Yergeau, Donald A. and Rosencrance, Celeste and Boggs, Kristy and Rusch, Michael C. and Ding, Liang and Zhang, Junyuan and Finkelstein, David and Noyes, Rachel M. and Russell, Brent L. and Xu, Beisi and Broniscer, Alberto and Wetmore, Cynthia and Pounds, Stanley B. and Ellison, David W. and Zhang, Jinghui and Baker, Suzanne J.},
