  file = {C\:\\Users\\jandrews\\Zotero\\storage\\XR4DWPHC\\Vigorito et al. - 2007 - microRNA-155 Regulates the Generation of Immunoglo.pdf;C\:\\Users\\jandrews\\Zotero\\storage\\9BMLEDXE\\S1074761307005031.html}
}

@article{vilagosEssentialRoleEBF12012,
  title = {Essential Role of {{EBF1}} in the Generation and Function of Distinct Mature {{B}} Cell Types},
  author = {Vilagos, Bojan and Hoffmann, Mareike and Souabni, Abdallah and Sun, Qiong and Werner, Barbara and Medvedovic, Jasna and Bilic, Ivan and Minnich, Martina and Axelsson, Elin and Jaritz, Markus and Busslinger, Meinrad},
  date = {2012-04-09},
