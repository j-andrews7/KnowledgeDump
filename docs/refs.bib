  shorttitle = {Epigenetic Down-Regulation of the Tumor Suppressor Gene {{PRDM1}}/{{Blimp-1}} in Diffuse Large {{B}} Cell Lymphomas},
  author = {Nie, Kui and Zhang, Taotao and Allawi, Hatim and Gomez, Mario and Liu, Yifang and Chadburn, Amy and Wang, Y. Lynn and Knowles, Daniel M. and Tam, Wayne},
