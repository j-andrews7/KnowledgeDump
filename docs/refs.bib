@article{sakaiGenomeWideTargetAnalyses2017,
  title = {Genome-{{Wide Target Analyses}} of {{Otx2 Homeoprotein}} in {{Postnatal Cortex}}},
  author = {Sakai, Akiko and Nakato, Ryuichiro and Ling, Yiwei and Hou, Xubin and Hara, Norikazu and Iijima, Tomoya and Yanagawa, Yuchio and Kuwano, Ryozo and Okuda, Shujiro and Shirahige, Katsuhiko and Sugiyama, Sayaka},
  date = {2017},
  journaltitle = {Frontiers in Neuroscience},
