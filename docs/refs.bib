@article{zhuAgedependentFateLineage2011,
  title = {Age-Dependent Fate and Lineage Restriction of Single {{NG2}} Cells},
  author = {Zhu, Xiaoqin and Hill, Robert A. and Dietrich, Dirk and Komitova, Mila and Suzuki, Ryusuke and Nishiyama, Akiko},
  date = {2011-02-15},
  journaltitle = {Development},
  volume = {138},
