  title = {Acquired Initiating Mutations in Early Hematopoietic Cells of {{CLL}} Patients},
  author = {Damm, Frederik and Mylonas, Elena and Cosson, Adrien and Yoshida, Kenichi and Valle, Veronique Della and Mouly, Enguerran and Diop, M'boyba and Scourzic, Laurianne and Shiraishi, Yuichi and Chiba, Kenichi and Tanaka, Hiroko and Miyano, Satoru and Kikushige, Yoshikane and Davi, Frederick and Lambert, Jerome and Gautheret, Daniel and Merle-Beral, Helene and Sutton, Laurent and Dessen, Philippe and Solary, Eric and Akashi, Koichi and Vainchenker, William and Mercher, Thomas and Droin, Nathalie and Ogawa, Seishi and Nguyen-Khac, Florence and Bernard, Olivier A.},
  date = {2014-06-11},
  journaltitle = {Cancer Discov},
  eprint = {24920063},
  eprinttype = {pmid},
  pages = {CD-14-0104},
  issn = {2159-8274, 2159-8290},
  doi = {10.1158/2159-8290.CD-14-0104},
  url = {http://cancerdiscovery.aacrjournals.org/content/early/2014/06/11/2159-8290.CD-14-0104},
  urldate = {2018-07-27},
  abstract = {Appropriate cancer care requires thorough understanding of the natural history of the disease, including the cell of origin, the clonal organization of the proliferation and the functional consequences of the mutations. Using deep sequencing of flow-sorted cell populations from chronic lymphocytic leukemia (CLL) patients, we established the presence of acquired mutations in multipotent hematopoietic progenitors of CLL patients. Mutations affected known lymphoid oncogenes including BRAF, NOTCH1 and SF3B1. NFKBIE and EGR2 mutations were observed at unexpected high frequencies, 10.7\% and 8.3\% of 168 advanced stage patients, respectively. EGR2 mutations were associated with a shorter time to treatment and poor overall survival. Analyses of BRAF and EGR2 mutation suggest they affect deregulation of BCR intracellular signaling. Our data propose deregulation of hematopoietic and early B-cell differentiation through the deregulation of pre-BCR signaling as a phenotypic convergence of CLL mutations and show that CLL develops from a pre-leukemic phase.},
  langid = {english},
  file = {C\:\\Users\\jandrews\\Zotero\\storage\\7FXIQK92\\Damm et al. - 2014 - Acquired initiating mutations in early hematopoiet.pdf;C\:\\Users\\jandrews\\Zotero\\storage\\PDLL8LMN\\2159-8290.CD-14-0104.html}
}

