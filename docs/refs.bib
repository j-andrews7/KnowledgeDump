  author = {Sayres, Melissa A. Wilson and Hauser, Charles and Sierk, Michael and Robic, Srebrenka and Rosenwald, Anne G. and Smith, Todd M. and Triplett, Eric W. and Williams, Jason J. and Dinsdale, Elizabeth and Morgan, William R. and Iii, James M. Burnette and Donovan, Samuel S. and Drew, Jennifer C. and Elgin, Sarah C. R. and Fowlks, Edison R. and Galindo-Gonzalez, Sebastian and Goodman, Anya L. and Grandgenett, Nealy F. and Goller, Carlos C. and Jungck, John R. and Newman, Jeffrey D. and Pearson, William and Ryder, Elizabeth F. and Tosado-Acevedo, Rafael and Tapprich, William and Tobin, Tammy C. and Toro-Martínez, Arlín and Welch, Lonnie R. and Wright, Robin and Barone, Lindsay and Ebenbach, David and McWilliams, Mindy and Olney, Kimberly C. and Pauley, Mark A.},
  date = {2018-06-05},
  journaltitle = {PLOS ONE},
  volume = {13},
