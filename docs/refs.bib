@article{solanaInnateImmunosenescenceEffect2012,
  title = {Innate Immunosenescence: {{Effect}} of Aging on Cells and Receptors of the Innate Immune System in Humans},
  shorttitle = {Innate Immunosenescence},
  author = {Solana, Rafael and Tarazona, Raquel and Gayoso, Inmaculada and Lesur, Olivier and Dupuis, Gilles and Fulop, Tamas},
  date = {2012-10},
  journaltitle = {Seminars in Immunology},
  series = {Aging of the {{Immune System}}},
  volume = {24},
  number = {5},
  pages = {331--341},
  issn = {1044-5323},
  doi = {10.1016/j.smim.2012.04.008},
  url = {http://www.sciencedirect.com/science/article/pii/S1044532312000474},
  urldate = {2016-03-10},
  abstract = {Components of the innate immune response, including neutrophils and macrophages, are the first line of defense against infections. Their role is to initiate an inflammatory response, phagocyte and kill pathogens, recruit natural killer cells (NK), and facilitate the maturation and migration of dendritic cells that will initiate the adaptive immune response. Extraordinary advances have been made in the last decade on the knowledge of the receptors and mechanisms used by cells of the innate immunity not only to sense and eliminate the pathogen but also to communicate each other and collaborate with cells of adaptive immunity to mount an effective immune response. The analysis of innate immunity in elderly humans has evidenced that aging has a profound impact on the phenotype and functions of these cells. Thus altered expression and/or function of innate immunity receptors and signal transduction leading to defective activation and decreased chemotaxis, phagocytosis and intracellular killing of pathogens have been described. The phenotype and function of NK cells from elderly individuals show significant changes that are compatible with remodeling of the different NK subsets, with a decrease in the CD56bright subpopulation and accumulation of the CD56dim cells, in particular those differentiated NK cells that co-express CD57, as well as a decreased expression of activating natural cytotoxicity receptors. These alterations can be responsible of the decreased production of cytokines and the lower per-cell cytotoxicity observed in the elderly. Considering the relevance of these cells in the initiation of the immune response, the possibility to reactivate the function of innate immune cells should be considered in order to improve the response to pathogens and to vaccination in the elderly.},
  keywords = {Dendritic cells,immunosenescence,Monocytes,Neutrophils,NK cells,Phagocytes},
  file = {C\:\\Users\\jandrews\\Zotero\\storage\\UUK4DAWJ\\Solana et al. - 2012 - Innate immunosenescence Effect of aging on cells .pdf;C\:\\Users\\jandrews\\Zotero\\storage\\I6PSUA2T\\S1044532312000474.html}
}

