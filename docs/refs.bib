@article{frankelPhenotypicRobustnessConferred2010,
  title = {Phenotypic Robustness Conferred by Apparently Redundant Transcriptional Enhancers},
  author = {Frankel, Nicolás and Davis, Gregory K. and Vargas, Diego and Wang, Shu and Payre, François and Stern, David L.},
  date = {2010-07-22},
  journaltitle = {Nature},
  volume = {466},
  number = {7305},
