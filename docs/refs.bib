@article{deAberrationDNAMethylation2013a,
  title = {Aberration in {{DNA Methylation}} in {{B-Cell Lymphomas Has}} a {{Complex Origin}} and {{Increases}} with {{Disease Severity}}},
  author = {De, Subhajyoti and Shaknovich, Rita and Riester, Markus and Elemento, Olivier and Geng, Huimin and Kormaksson, Matthias and Jiang, Yanwen and Woolcock, Bruce and Johnson, Nathalie and Polo, Jose M. and Cerchietti, Leandro and Gascoyne, Randy D. and Melnick, Ari and Michor, Franziska},
  date = {2013-01-10},
  journaltitle = {PLoS Genet},
  volume = {9},
