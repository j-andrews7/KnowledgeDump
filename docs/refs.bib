  author = {Tonack, S. and Jenkinson, C. and Cox, T. and Elliott, V. and Jenkins, R. E. and Kitteringham, N. R. and Greenhalf, W. and Shaw, V. and Michalski, C. W. and Friess, H. and Neoptolemos, J. P. and Costello, E.},
  date = {2013-03-14},
  journaltitle = {Br J Cancer},
