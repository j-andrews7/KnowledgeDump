  author = {Chakraborty, Arup R. and Robey, Robert W. and Luchenko, Victoria L. and Zhan, Zhirong and Piekarz, Richard L. and Gillet, Jean-Pierre and Kossenkov, Andrew V. and Wilkerson, Julia and Showe, Louise C. and Gottesman, Michael M. and Collie, Nathan L. and Bates, Susan E.},
  date = {2013-05-16},
  journaltitle = {Blood},
