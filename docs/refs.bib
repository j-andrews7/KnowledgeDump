@article{levyClinicalIssuesResearch2005,
  title = {Clinical {{Issues}} and {{Research}} in {{Respiratory Failure}} from {{Severe Acute Respiratory Syndrome}}},
  author = {Levy, Mitchell M. and Baylor, Melisse S. and Bernard, Gordon R. and Fowler, Rob and Franks, Teri J. and Hayden, Frederick G. and Helfand, Rita and Lapinsky, Stephen E. and Martin, Thomas R. and Niederman, Michael S. and Rubenfeld, Gordon D. and Slutsky, Arthur S. and Stewart, Thomas E. and Styrt, Barbara A. and Thompson, B. Taylor and Harabin, Andrea L.},
  date = {2005-03-01},
  journaltitle = {Am J Respir Crit Care Med},
