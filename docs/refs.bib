  shorttitle = {A Silver Lining in Cell Line Authentication},
  author = {Gu, Meijia and Yang, Meimei and He, Jing and Xia, Sixuan and Zhang, Zhe and Wang, Yudong and Zheng, Congyi and Shen, Chao},
