@article{armitageIL15HasStimulatory1995,
  title = {{{IL-15}} Has Stimulatory Activity for the Induction of {{B}} Cell Proliferation and Differentiation.},
  author = {Armitage, R. J. and Macduff, B. M. and Eisenman, J. and Paxton, R. and Grabstein, K. H.},
  date = {1995-01-15},
  journaltitle = {J Immunol},
