  title = {{{{\emph{DNMT3A}}}}{{{\textsuperscript{{\emph{R882H}}}}}} Mutant and {{{\emph{Tet2}}}} Inactivation Cooperate in the Deregulation of {{DNA}} Methylation Control to Induce Lymphoid Malignancies in Mice},
  author = {Scourzic, L. and Couronné, L. and Pedersen, M. T. and Valle, V. Della and Diop, M. and Mylonas, E. and Calvo, J. and Mouly, E. and Lopez, C. K. and Martin, N. and Fontenay, M. and Bender, A. and Guibert, S. and Dubreuil, P. and Dessen, P. and Droin, N. and Pflumio, F. and Weber, M. and Gaulard, P. and Helin, K. and Mercher, T. and Bernard, O. A.},
  date = {2016-06},
  journaltitle = {Leukemia},
  volume = {30},
