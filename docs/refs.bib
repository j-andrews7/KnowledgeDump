@article{liuFcuReceptorPromotes2018,
  title = {Fcµ {{Receptor Promotes}} the {{Survival}} and {{Activation}} of {{Marginal Zone B Cells}} and {{Protects Mice}} against {{Bacterial Sepsis}}},
  author = {Liu, Jun and Zhu, Hanying and Qian, Jiawen and Xiong, Ermeng and Zhang, Lumin and Wang, Yan-Qing and Chu, Yiwei and Kubagawa, Hiromi and Tsubata, Takeshi and Wang, Ji-Yang},
  date = {2018-02-05},
  journaltitle = {Front Immunol},
  volume = {9},
  eprint = {29459869},
  eprinttype = {pmid},
  issn = {1664-3224},
  doi = {10.3389/fimmu.2018.00160},
  url = {https://www.ncbi.nlm.nih.gov/pmc/articles/PMC5807594/},
  urldate = {2018-03-02},
  abstract = {The marginal zone B cells (MZB) are located at the interface between the circulation and lymphoid tissue and as a gatekeeper play important roles in both innate and adaptive immune responses. We have previously found that MZB are significantly reduced in mice deficient in the IgM Fc receptor (FcμR) but how FcμR regulates the development and function of MZB remains unknown. In this study, we found that both marginal zone precursor (MZP) and MZB were decreased in FcμR−/− mice. The reduction of MZP and MZB was not due to impaired proliferation of these cells but rather due to their increased death. Further analysis revealed that FcμR−/− MZB had reduced tonic BCR signal, as evidenced by their decreased levels of phosphorylated SYK and AKT relative to WT MZB. MZB in FcμR−/− mice responded poorly to LPS in vivo when compared with MZB in WT mice. Consistent with the reduced proportion of MZB and their impaired response to LPS, antibody production against the type 1 T-independent Ag, NP-LPS, was significantly reduced in FcμR−/− mice. Moreover, FcμR−/− mice were highly susceptible to Citrobacter rodentium-induced sepsis. These results reveal a critical role for FcμR in the survival and activation of MZB and in protection against acute bacterial infection.},
  pmcid = {PMC5807594},
  file = {C:\Users\jandrews\Zotero\storage\R7LI6PTD\Liu et al. - 2018 - Fcµ Receptor Promotes the Survival and Activation .pdf}
}

