@article{adraLAPTM5NovelLysosomalAssociated1996,
  title = {{{LAPTM5}}: {{A Novel Lysosomal-Associated Multispanning Membrane Protein Preferentially Expressed}} in {{Hematopoietic Cells}}},
  shorttitle = {{{LAPTM5}}},
  author = {Adra, Chaker N. and Zhu, Shaochun and Ko, Jone-Long and Guillemot, Jean-Claude and Cuervo, Ana Maria and Kobayashi, Hirofumi and Horiuchi, Tadashi and Lelias, Jean-Michel and Rowley, Janet D. and Lim, Bing},
  date = {1996-07-15},
  journaltitle = {Genomics},
  volume = {35},
  number = {2},
  pages = {328--337},
  issn = {0888-7543},
  doi = {10.1006/geno.1996.0364},
  url = {http://www.sciencedirect.com/science/article/pii/S088875439690364X},
  urldate = {2015-08-14},
  abstract = {While a large body of knowledge about cell membrane proteins exists, much less is known about the repertoire and function of integral membrane proteins of intracellular organelles. In looking for novel classes of genes that are functionally important to hematopoietic cells, we have cloned the cDNA for a gene preferentially expressed in adult hematopoietic tissues. During embryonic development the gene is expressed in both hematopoietic and nonhematopoietic tissues. In cell lines the gene is expressed specifically in hematopoietic lineages, whereas in normal adult tissues the mRNA is preferentially detected at high levels in lymphoid and myeloid tissues. The predicted protein is a pentaspanner with no homology to known genes and conserved across evolution. Immunocytological and cell fractionation studies with a specific antibody revealed a protein localizing in lysosomes. The gene, provisionally named LAPTM5, maps to chromosome 1p34. The expression pattern of the gene together with preliminary evidence that the protein interacts with ubiquitin indicates that the protein may have a special functional role during embryogenesis and in adult hematopoietic cells.},
  file = {C\:\\Users\\jandrews\\Zotero\\storage\\GZ5U5ET7\\Adra et al. - 1996 - LAPTM5 A Novel Lysosomal-Associated Multispanning.pdf;C\:\\Users\\jandrews\\Zotero\\storage\\MT7DU2P3\\S088875439690364X.html}
}

