@article{hodzicRoleMucosalImmunity2017,
  title = {The {{Role}} of {{Mucosal Immunity}} in the {{Pathogenesis}} of {{Necrotizing Enterocolitis}}},
  author = {Hodzic, Zerina and Bolock, Alexa M. and Good, Misty},
  date = {2017},
  journaltitle = {Front. Pediatr.},
