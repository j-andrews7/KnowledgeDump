  author = {Saito, Masumichi and Gao, Jie and Basso, Katia and Kitagawa, Yukiko and Smith, Paula M. and Bhagat, Govind and Pernis, Alessandra and Pasqualucci, Laura and Dalla-Favera, Riccardo},
