  author = {Fantin, Valeria R. and Loboda, Andrey and Paweletz, Cloud P. and Hendrickson, Ronald C. and Pierce, Jacqueline W. and Roth, Jennifer A. and Li, Lixia and Gooden, Frank and Korenchuk, Susan and Hou, Xiaoli S. and Harrington, Elizabeth A. and Randolph, Sophia and Reilly, John F. and Ware, Christopher M. and Kadin, Marshall E. and Frankel, Stanley R. and Richon, Victoria M.},
  date = {2008-05-15},
  journaltitle = {Cancer Res},
  volume = {68},
