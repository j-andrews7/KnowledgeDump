@article{wongEvolvingInsightsPathogenesis2011,
  title = {Evolving {{Insights}} in the {{Pathogenesis}} and {{Therapy}} of {{Cutaneous T-cell}} Lymphoma ({{Mycosis Fungoides}} and {{Sezary Syndrome}})},
