  author = {Ito, Mitsugu and Teshima, Kazuaki and Ikeda, Sho and Kitadate, Akihiro and Watanabe, Atsushi and Nara, Miho and Yamashita, Junsuke and Ohshima, Koichi and Sawada, Kenichi and Tagawa, Hiroyuki},
  date = {2014-01-01},
  journaltitle = {Blood},
  eprint = {24385540},
  eprinttype = {pmid},
