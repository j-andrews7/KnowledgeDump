@article{winkleEmergingRolesLong2017,
  title = {Emerging Roles for Long Noncoding {{RNAs}} in {{B-cell}} Development and Malignancy},
  author = {Winkle, M. and Kluiver, J. L. and Diepstra, A. and van den Berg, A.},
  options = {useprefix=true},
  date = {2017-12-01},
  journaltitle = {Critical Reviews in Oncology/Hematology},
