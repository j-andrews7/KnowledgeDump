@article{brownNFkBDirectsDynamic2014,
  title = {{{NF-κB Directs Dynamic Super Enhancer Formation}} in {{Inflammation}} and {{Atherogenesis}}},
  author = {Brown, Jonathan~D. and Lin, Charles~Y. and Duan, Qiong and Griffin, Gabriel and Federation, Alexander J. and Paranal, Ronald~M. and Bair, Steven and Newton, Gail and Lichtman, Andrew H. and Kung, Andrew L. and Yang, Tianlun and Wang, Hong and Luscinskas, Francis~W. and Croce, Kevin J. and Bradner, James~E. and Plutzky, Jorge},
  date = {2014-10-23},
  journaltitle = {Molecular Cell},
