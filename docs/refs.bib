@article{agarwalCelltyperestrictedBindingTranscription2000,
  title = {Cell-Type-Restricted Binding of the Transcription Factor {{NFAT}} to a Distal {{IL-4}} Enhancer in Vivo},
  author = {Agarwal, S. and Avni, O. and Rao, A.},
  date = {2000-06},
  journaltitle = {Immunity},
  volume = {12},
  number = {6},
