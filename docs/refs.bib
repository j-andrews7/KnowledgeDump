@article{qianCellSuperEnhancersRegulatory2014,
  title = {B {{Cell Super-Enhancers}} and {{Regulatory Clusters Recruit AID Tumorigenic Activity}}},
  author = {Qian, Jason and Wang, Qiao and Dose, Marei and Pruett, Nathanael and Kieffer-Kwon, Kyong-Rim and Resch, Wolfgang and Liang, Genqing and Tang, Zhonghui and Mathé, Ewy and Benner, Christopher and Dubois, Wendy and Nelson, Steevenson and Vian, Laura and Oliveira, Thiago~Y. and Jankovic, Mila and Hakim, Ofir and Gazumyan, Anna and Pavri, Rushad and Awasthi, Parirokh and Song, Bin and Liu, Geng and Chen, Longyun and Zhu, Shida and Feigenbaum, Lionel and Staudt, Louis and Murre, Cornelis and Ruan, Yijun and Robbiani, Davide~F. and Pan-Hammarström, Qiang and Nussenzweig, Michel~C. and Casellas, Rafael},
  date = {2014-12-18},
  journaltitle = {Cell},
  volume = {159},
  number = {7},
  pages = {1524--1537},
  issn = {0092-8674},
  doi = {10.1016/j.cell.2014.11.013},
  url = {http://www.sciencedirect.com/science/article/pii/S0092867414014445},
  urldate = {2018-05-14},
  abstract = {Summary The antibody gene mutator activation-induced cytidine deaminase (AID) promiscuously damages oncogenes, leading to chromosomal translocations and~tumorigenesis. Why nonimmunoglobulin loci are susceptible to AID activity is unknown. Here, we study AID-mediated lesions in the context of nuclear architecture and the B cell regulome. We show that AID targets are not randomly distributed across the~genome but are predominantly grouped within super-enhancers and regulatory clusters. Unexpectedly, in these domains, AID deaminates active promoters and eRNA+ enhancers interconnected in some instances over megabases of linear chromatin. Using genome editing, we demonstrate that 3D-linked targets cooperate to recruit AID-mediated breaks. Furthermore, a comparison of hypermutation in mouse B cells, AID-induced kataegis in human lymphomas, and translocations in MEFs reveals that AID damages different genes in different cell types. Yet, in all cases, the targets are predominantly associated with topological complex, highly transcribed super-enhancers, demonstrating that these compartments are key mediators of AID recruitment.},
  file = {C\:\\Users\\jandrews\\Zotero\\storage\\LPRBC52B\\Qian et al. - 2014 - B Cell Super-Enhancers and Regulatory Clusters Rec.pdf;C\:\\Users\\jandrews\\Zotero\\storage\\UE8D45PP\\S0092867414014445.html}
}

