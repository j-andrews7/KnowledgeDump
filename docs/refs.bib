}

@article{caronCellCycleDependentReconfigurationDNA2015,
  title = {Cell-{{Cycle-Dependent Reconfiguration}} of the {{DNA Methylome}} during {{Terminal Differentiation}} of {{Human B Cells}} into {{Plasma Cells}}},
  author = {Caron, Gersende and Hussein, Mourad and Kulis, Marta and Delaloy, Céline and Chatonnet, Fabrice and Pignarre, Amandine and Avner, Stéphane and Lemarié, Maud and Mahé, Elise~A. and Verdaguer-Dot, Núria and Queirós, Ana~C. and Tarte, Karin and Martín-Subero, José~I. and Salbert, Gilles and Fest, Thierry},
  date = {2015-11-03},
  journaltitle = {Cell Reports},
  volume = {13},
