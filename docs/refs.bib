@article{herlingHighTCL1Levels2009,
  title = {High {{TCL1}} Levels Are a Marker of {{B-cell}} Receptor Pathway Responsiveness and Adverse Outcome in Chronic Lymphocytic Leukemia},
  author = {Herling, Marco and Patel, Kaushali A. and Weit, Nicole and Lilienthal, Nils and Hallek, Michael and Keating, Michael J. and Jones, Dan},
  date = {2009-11-19},
  journaltitle = {Blood},
  volume = {114},
  number = {21},
  eprint = {19770358},
  eprinttype = {pmid},
  pages = {4675--4686},
  issn = {0006-4971, 1528-0020},
  doi = {10.1182/blood-2009-03-208256},
  url = {http://www.bloodjournal.org/content/114/21/4675},
  urldate = {2015-08-11},
  abstract = {Although activation of the B-cell receptor (BCR) signaling pathway is implicated in the pathogenesis of chronic lymphocytic leukemia (CLL), its clinical impact and the molecular correlates of such response are not clearly defined. T-cell leukemia 1 (TCL1), the AKT modulator and proto-oncogene, is differentially expressed in CLL and linked to its pathogenesis based on CD5+ B-cell expansions arising in TCL1-transgenic mice. We studied here the association of TCL1 levels and its intracellular dynamics with the in vitro responses to BCR stimulation in 70 CLL cases. The growth kinetics after BCR engagement correlated strongly with the degree and timing of induced AKT phospho-activation. This signaling intensity was best predicted by TCL1 levels and the kinetics of TCL1-AKT corecruitment to BCR membrane activation complexes, which further included the kinases LYN, SYK, ZAP70, and PKC. High TCL1 levels were also strongly associated with aggressive disease features, such as advanced clinical stage, higher white blood cell counts, and shorter lymphocyte doubling time. Higher TCL1 levels independently predicted an inferior clinical outcome (ie, shorter progression-free survival, P {$<$} .001), regardless of therapy regimen, especially for ZAP70+ tumors. We propose TCL1 as a marker of the BCR-responsive CLL subset identifying poor prognostic cases where targeting BCR-associated kinases may be therapeutically useful.},
  langid = {english},
  file = {C\:\\Users\\jandrews\\Zotero\\storage\\4CVH92SK\\Herling et al. - 2009 - High TCL1 levels are a marker of B-cell receptor p.pdf;C\:\\Users\\jandrews\\Zotero\\storage\\JHUM2MPW\\4675.html}
}

@article{herranzNOTCH1drivenMYCEnhancer2014,
  title = {A {{NOTCH1-driven MYC}} Enhancer Promotes {{T}} Cell Development, Transformation and Acute Lymphoblastic Leukemia},
  author = {Herranz, Daniel and Ambesi-Impiombato, Alberto and Palomero, Teresa and Schnell, Stephanie A. and Belver, Laura and Wendorff, Agnieszka A. and Xu, Luyao and Castillo-Martin, Mireia and Llobet-Navás, David and Cordon-Cardo, Carlos and Clappier, Emmanuelle and Soulier, Jean and Ferrando, Adolfo A.},
  date = {2014-10},
  journaltitle = {Nat Med},
  volume = {20},
  number = {10},
  pages = {1130--1137},
  issn = {1078-8956},
  doi = {10.1038/nm.3665},
  url = {http://www.nature.com/nm/journal/v20/n10/full/nm.3665.html},
  urldate = {2015-07-13},
  abstract = {Efforts to identify and annotate cancer driver genetic lesions have been focused primarily on the analysis of protein-coding genes; however, most genetic abnormalities found in human cancer are located in intergenic regions. Here we identify a new long range–acting MYC enhancer controlled by NOTCH1 that is targeted by recurrent chromosomal duplications in human T cell acute lymphoblastic leukemia (T-ALL). This highly conserved regulatory element, hereby named N-Me for NOTCH MYC enhancer, is located within a broad super-enhancer region +1.47 Mb from the MYC transcription initiating site, interacts with the MYC proximal promoter and induces orientation-independent MYC expression in reporter assays. Moreover, analysis of N-Me knockout mice demonstrates a selective and essential role of this regulatory element during thymocyte development and in NOTCH1-induced T-ALL. Together these results identify N-Me as a long-range oncogenic enhancer implicated directly in the pathogenesis of human leukemia and highlight the importance of the NOTCH1-MYC regulatory axis in T cell transformation and as a therapeutic target in T-ALL.},
  langid = {english},
  file = {C\:\\Users\\jandrews\\Zotero\\storage\\XCHJR3GQ\\Herranz et al. - 2014 - A NOTCH1-driven MYC enhancer promotes T cell devel.pdf;C\:\\Users\\jandrews\\Zotero\\storage\\W8BJ8WQ6\\nm.3665.html}
}

@article{herringHumanPrefrontalCortex2022,
  title = {Human Prefrontal Cortex Gene Regulatory Dynamics from Gestation to Adulthood at Single-Cell Resolution},
  author = {Herring, Charles A. and Simmons, Rebecca K. and Freytag, Saskia and Poppe, Daniel and Moffet, Joel J. D. and Pflueger, Jahnvi and Buckberry, Sam and Vargas-Landin, Dulce B. and Clément, Olivier and Echeverría, Enrique Goñi and Sutton, Gavin J. and Alvarez-Franco, Alba and Hou, Rui and Pflueger, Christian and McDonald, Kerrie and Polo, Jose M. and Forrest, Alistair R. R. and Nowak, Anna K. and Voineagu, Irina and Martelotto, Luciano and Lister, Ryan},
  date = {2022-11-10},
  journaltitle = {Cell},
  volume = {185},
  number = {23},
  pages = {4428-4447.e28},
  issn = {0092-8674},
  doi = {10.1016/j.cell.2022.09.039},
  url = {https://www.sciencedirect.com/science/article/pii/S0092867422012582},
  urldate = {2023-03-07},
  abstract = {Human brain development is underpinned by cellular and molecular reconfigurations continuing into the third decade of life. To reveal cell dynamics orchestrating neural maturation, we profiled human prefrontal cortex gene expression and chromatin accessibility at single-cell resolution from gestation to adulthood. Integrative analyses define the dynamic trajectories of each cell type, revealing major gene expression reconfiguration at the prenatal-to-postnatal transition in all cell types followed by continuous reconfiguration into adulthood and identifying regulatory networks guiding cellular developmental programs, states, and functions. We uncover links between expression dynamics and developmental milestones, characterize the diverse timing of when cells acquire adult-like states, and identify molecular convergence from distinct developmental origins. We further reveal cellular dynamics and their regulators implicated in neurological disorders. Finally, using this reference, we benchmark cell identities and maturation states in organoid models. Together, this captures the dynamic regulatory landscape of human cortical development.},
  langid = {english},
  keywords = {brain disorders,cerebral organoids,development,single-cell ATAC-seq,single-cell RNA-seq},
  file = {C\:\\Users\\jandrews\\Zotero\\storage\\9H9E9W7L\\Herring et al. - 2022 - Human prefrontal cortex gene regulatory dynamics f.pdf;C\:\\Users\\jandrews\\Zotero\\storage\\Q8TYBENP\\S0092867422012582.html}
}

