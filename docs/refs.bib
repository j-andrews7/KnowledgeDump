@article{lobanenkovNovelSequencespecificDNA1990,
  title = {A Novel Sequence-Specific {{DNA}} Binding Protein Which Interacts with Three Regularly Spaced Direct Repeats of the {{CCCTC-motif}} in the 5'-Flanking Sequence of the Chicken c-Myc Gene},
  author = {Lobanenkov, V. V. and Nicolas, R. H. and Adler, V. V. and Paterson, H. and Klenova, E. M. and Polotskaja, A. V. and Goodwin, G. H.},
  date = {1990-12},
  journaltitle = {Oncogene},
