@article{hatziHybridMechanismAction2013,
  title = {A Hybrid Mechanism of Action for {{BCL6}} in {{B-cells}} Defined by Formation of Functionally Distinct Complexes at Enhancers and Promoters},
  author = {Hatzi, Katerina and Jiang, Yanwen and Huang, Chuanxin and Garrett-Bakelman, Francine and Gearhart, Micah D. and Giannopoulou, Eugenia G. and Zumbo, Paul and Kirouac, Kevin and Bhaskara, Srividya and Polo, Jose M. and Kormaksson, Matthias and MacKerell, Alexander D. and Xue, Fengtian and Mason, Christopher E. and Hiebert, Scott W. and Prive, Gilbert G. and Cerchietti, Leandro and Bardwell, Vivian J. and Elemento, Olivier and Melnick, Ari},
  date = {2013-08-15},
  journaltitle = {Cell Rep},
  volume = {4},
  number = {3},
  eprint = {23911289},
  eprinttype = {pmid},
  issn = {2211-1247},
  doi = {10.1016/j.celrep.2013.06.016},
  url = {http://www.ncbi.nlm.nih.gov/pmc/articles/PMC3854650/},
  urldate = {2016-09-16},
  abstract = {The BCL6 transcriptional repressor is required for development of germinal center (GC) B-cells and diffuse large B-cell lymphomas (DLBCL). Although BCL6 can recruit multiple corepressors, its transcriptional repression mechanism of action in normal and malignant B-cells is unknown. We find that in B-cells, BCL6 mostly functions through two independent mechanisms that are collectively essential to GC formation and DLBCL, both mediated through its N-terminal BTB domain. These are: i) formation of a unique ternary BCOR-SMRT complex at promoters with each corepressor binding to symmetrical sites on BCL6 homodimers, linked to specific epigenetic chromatin features, and ii) the “toggling” of active enhancers to a poised but not erased conformation through SMRT-dependent H3K27 de-acetylation, which is mediated by HDAC3 and opposed by p300 histone acetyltransferase. Dynamic toggling of enhancers provides a basis for B-cells to undergo rapid transcriptional and phenotypic changes in response to signaling or environmental cues.},
  pmcid = {PMC3854650},
  file = {C:\Users\jandrews\Zotero\storage\494KZ7DG\Hatzi et al. - 2013 - A hybrid mechanism of action for BCL6 in B-cells d.pdf}
}

