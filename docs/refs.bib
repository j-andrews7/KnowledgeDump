@article{niuProteinstructureguidedDiscoveryFunctional2016,
  title = {Protein-Structure-Guided Discovery of Functional Mutations across 19 Cancer Types},
  author = {Niu, Beifang and Scott, Adam D. and Sengupta, Sohini and Bailey, Matthew H. and Batra, Prag and Ning, Jie and Wyczalkowski, Matthew A. and Liang, Wen-Wei and Zhang, Qunyuan and McLellan, Michael D. and Sun, Sam Q. and Tripathi, Piyush and Lou, Carolyn and Ye, Kai and Mashl, R. Jay and Wallis, John and Wendl, Michael C. and Chen, Feng and Ding, Li},
  date = {2016-06-13},
