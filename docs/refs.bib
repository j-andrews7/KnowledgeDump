@article{dengControllingLongRangeGenomic2012,
  title = {Controlling {{Long-Range Genomic Interactions}} at a {{Native Locus}} by {{Targeted Tethering}} of a {{Looping Factor}}},
  author = {Deng, Wulan and Lee, Jongjoo and Wang, Hongxin and Miller, Jeff and Reik, Andreas and Gregory, Philip D. and Dean, Ann and Blobel, Gerd A.},
