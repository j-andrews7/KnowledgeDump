@article{zengMolecularBasisIL212007,
  title = {The Molecular Basis of {{IL-21}}–Mediated Proliferation},
  author = {Zeng, Rong and Spolski, Rosanne and Casas, Esther and Zhu, Wei and Levy, David E. and Leonard, Warren J.},
  date = {2007-05-15},
  journaltitle = {Blood},
  volume = {109},
  number = {10},
  eprint = {17234735},
