@article{velmeshevSinglecellAnalysisPrenatal2023,
  title = {Single-Cell Analysis of Prenatal and Postnatal Human Cortical Development},
  author = {Velmeshev, Dmitry and Perez, Yonatan and Yan, Zihan and Valencia, Jonathan E. and Castaneda-Castellanos, David R. and Wang, Li and Schirmer, Lucas and Mayer, Simone and Wick, Brittney and Wang, Shaohui and Nowakowski, Tomasz Jan and Paredes, Mercedes and Huang, Eric J. and Kriegstein, Arnold R.},
