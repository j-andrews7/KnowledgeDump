@article{kieffer-kwonMycRegulatesChromatin2017,
  title = {Myc {{Regulates Chromatin Decompaction}} and {{Nuclear Architecture}} during {{B Cell Activation}}},
  author = {Kieffer-Kwon, Kyong-Rim and Nimura, Keisuke and Rao, Suhas S. P. and Xu, Jianliang and Jung, Seolkyoung and Pekowska, Aleksandra and Dose, Marei and Stevens, Evan and Mathe, Ewy and Dong, Peng and Huang, Su-Chen and Ricci, Maria Aurelia and Baranello, Laura and Zheng, Ying and Tomassoni Ardori, Francesco and Resch, Wolfgang and Stavreva, Diana and Nelson, Steevenson and McAndrew, Michael and Casellas, Adriel and Finn, Elizabeth and Gregory, Charles and St. Hilaire, Brian Glenn and Johnson, Steven M. and Dubois, Wendy and Cosma, Maria Pia and Batchelor, Eric and Levens, David and Phair, Robert D. and Misteli, Tom and Tessarollo, Lino and Hager, Gordon and Lakadamyali, Melike and Liu, Zhe and Floer, Monique and Shroff, Hari and Aiden, Erez Lieberman and Casellas, Rafael},
  date = {2017-08-17},
  journaltitle = {Molecular Cell},
  volume = {67},
  number = {4},
  pages = {566-578.e10},
  issn = {1097-2765},
  doi = {10.1016/j.molcel.2017.07.013},
  url = {http://www.sciencedirect.com/science/article/pii/S1097276517305075},
  urldate = {2018-07-11},
  abstract = {Summary 50 years ago, Vincent Allfrey and colleagues discovered that lymphocyte activation triggers massive acetylation of chromatin. However, the molecular mechanisms driving epigenetic accessibility are still unknown. We here show that stimulated lymphocytes decondense chromatin by three differentially regulated steps. First, chromatin is repositioned away from the nuclear periphery in response to global acetylation. Second, histone nanodomain clusters decompact into mononucleosome fibers through a mechanism that requires Myc and continual energy input. Single-molecule imaging shows that this step lowers transcription factor residence time and non-specific collisions during sampling for DNA targets. Third, chromatin interactions shift from long range to predominantly short range, and CTCF-mediated loops and contact domains double in numbers. This architectural change facilitates cognate promoter-enhancer contacts and also requires Myc and continual ATP production. Our results thus define the nature and transcriptional impact of chromatin decondensation and reveal an unexpected role for Myc in the establishment of nuclear topology in mammalian cells.},
  keywords = {B cells,chromatin remodeling,cohesin,CTCF,Histone acetylation,immune response,myc,nanoscopy,nuclear architecture,transcriptome amplification},
  file = {C\:\\Users\\jandrews\\Zotero\\storage\\IRELJFU2\\Kieffer-Kwon et al. - 2017 - Myc Regulates Chromatin Decompaction and Nuclear A.pdf;C\:\\Users\\jandrews\\Zotero\\storage\\TWFRXCWN\\S1097276517305075.html}
}

@article{kielGenomicAnalysesReveal2015,
  title = {Genomic Analyses Reveal Recurrent Mutations in Epigenetic Modifiers and the {{JAK-STAT}} Pathway in {{Sezary}} Syndrome},
  author = {Kiel, Mark J. and Sahasrabuddhe, Anagh A. and Rolland, Delphine C. M. and Velusamy, Thirunavukkarasu and Chung, Fuzon and Schaller, Matthew and Bailey, Nathanael G. and Betz, Bryan L. and Miranda, Roberto N. and Porcu, Pierluigi and Byrd, John C. and Jeffrey Medeiros, L. and Kunkel, Steven L. and Bahler, David W. and Lim, Megan S. and Elenitoba-Johnson, Kojo S. J.},
  date = {2015-09-29},
  journaltitle = {Nat Commun},
  volume = {6},
  pages = {8470},
  doi = {10.1038/ncomms9470},
  url = {http://www.nature.com/ncomms/2015/150929/ncomms9470/full/ncomms9470.html},
  urldate = {2016-08-09},
  abstract = {Sézary syndrome (SS) is an aggressive leukaemia of mature T cells with poor prognosis and limited options for targeted therapies. The comprehensive genetic alterations underlying the pathogenesis of SS are unknown. Here we integrate whole-genome sequencing (n=6), whole-exome sequencing (n=66) and array comparative genomic hybridization-based copy-number analysis (n=80) of primary SS samples. We identify previously unknown recurrent loss-of-function aberrations targeting members of the chromatin remodelling/histone modification and trithorax families, including ARID1A in which functional loss from nonsense and frameshift mutations and/or targeted deletions is observed in 40.3\% of SS genomes. We also identify recurrent gain-of-function mutations targeting PLCG1 (9\%) and JAK1, JAK3, STAT3 and STAT5B (JAK/STAT total \textasciitilde 11\%). Functional studies reveal sensitivity of JAK1-mutated primary SS cells to JAK inhibitor treatment. These results highlight the complex genomic landscape of SS and a role for inhibition of JAK/STAT pathways for the treatment of SS.},
  langid = {english},
  keywords = {Biological sciences,cancer,Genetics},
  file = {C\:\\Users\\jandrews\\Zotero\\storage\\9KZCZR5S\\Kiel et al. - 2015 - Genomic analyses reveal recurrent mutations in epi.pdf;C\:\\Users\\jandrews\\Zotero\\storage\\4P8AN3BH\\ncomms9470.html}
}

