@article{wangHiNTComputationalMethod2020,
  title = {{{HiNT}}: A Computational Method for Detecting Copy Number Variations and Translocations from {{Hi-C}} Data},
  shorttitle = {{{HiNT}}},
  author = {Wang, Su and Lee, Soohyun and Chu, Chong and Jain, Dhawal and Kerpedjiev, Peter and Nelson, Geoffrey M. and Walsh, Jennifer M. and Alver, Burak H. and Park, Peter J.},
