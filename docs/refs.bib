  langid = {english},
  keywords = {Cancer metabolism,Cancer microenvironment,CNS cancer,Tumour biomarkers},
  file = {C:\Users\jandrews\Zotero\storage\FEWGGC6N\Coy et al. - 2022 - Single cell spatial analysis reveals the topology .pdf}
