@article{berberichCrosslinkingCD40Cells1994,
  title = {Cross-Linking {{CD40}} on {{B}} Cells Rapidly Activates Nuclear Factor-Kappa {{B}}.},
  author = {Berberich, I. and Shu, G. L. and Clark, E. A.},
  date = {1994-11-15},
  journaltitle = {J Immunol},
  volume = {153},
  number = {10},
