@article{pepperOpposingSignalsBcl62011,
  title = {Opposing {{Signals}} from the {{Bcl6 Transcription Factor}} and the {{Interleukin-2 Receptor Generate T Helper}} 1 {{Central}} and {{Effector Memory Cells}}},
