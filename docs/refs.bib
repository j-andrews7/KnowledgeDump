@article{hayamiInactivationE3LAPTm52003,
  title = {Inactivation of the {{E3}}/{{LAPTm5}} Gene by Chromosomal Rearrangement and {{DNA}} Methylation in Human Multiple Myeloma},
  author = {Hayami, Y. and Iida, S. and Nakazawa, N. and Hanamura, I. and Kato, M. and Komatsu, H. and Miura, I. and Dave, B. J. and Sanger, W. G. and Lim, B. and Taniwaki, M. and Ueda, R.},
  date = {2003},
  journaltitle = {Leukemia},
  volume = {17},
  number = {8},
  pages = {1650--1657},
  issn = {0887-6924},
  doi = {10.1038/sj.leu.2403026},
  url = {http://www.nature.com/leu/journal/v17/n8/full/2403026a.html},
  urldate = {2015-08-14},
  abstract = {Chromosomal band 1p34–36 is a commonly rearranged locus in many types of cancers. We cloned the breakpoint region of a chromosomal translocation, t(1;14)(p34;q32), found in the human multiple myeloma (MM) cell line, ODA. This rearrangement occurred between the nearby switch region of the immunoglobulin heavy chain (IgH) gene (S3) at 14q32 and the first intron of the human retinoic acid-inducible E3 protein (E3)/lysosome-associated protein, transmembrane-5 (LAPTm5) gene at the 1p34 locus. Consequently, the E3 gene, which is a hematopoietic cell-specific transcript induced by retinoic acid and located at the rearranged allele, was interrupted within its coding region and was not expressed in the ODA cell line in spite of the other allele still being intact. The expression derived from the remaining intact allele in ODA cells was silenced by DNA methylation at sequences within the first intron around a GC-rich EagI site. Interestingly, the silenced expression of E3 mRNA due to DNA methylation of intron 1 sequences was frequently encountered in MM cells [6/10 (60\%) of MM cell lines tested], while E3 is expressed in normal plasma cells and in most other hematopoietic cell lines including those of B-cell lineage. Thus, as the E3 protein has been suggested to be involved in cellular differentiation and apoptotic pathways in certain cell types, our results suggest that loss of E3 gene expression might be a crucial event during the progression of human MM.},
  langid = {english},
  keywords = {DNA methylation,E3/LAPTm5,multiple myeloma,q32),t(1;14)(p34},
  file = {C\:\\Users\\jandrews\\Zotero\\storage\\XCVACBNN\\Hayami et al. - 2003 - Inactivation of the E3LAPTm5 gene by chromosomal .pdf;C\:\\Users\\jandrews\\Zotero\\storage\\MSCMIWNF\\2403026a.html}
}

