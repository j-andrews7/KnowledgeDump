@article{liuLandscapeTumorCell2022,
  title = {The Landscape of Tumor Cell States and Spatial Organization in {{H3-K27M}} Mutant Diffuse Midline Glioma across Age and Location},
  author = {Liu, Ilon and Jiang, Li and Samuelsson, Erik R. and Marco Salas, Sergio and Beck, Alexander and Hack, Olivia A. and Jeong, Daeun and Shaw, McKenzie L. and Englinger, Bernhard and LaBelle, Jenna and Mire, Hafsa M. and Madlener, Sibylle and Mayr, Lisa and Quezada, Michael A. and Trissal, Maria and Panditharatna, Eshini and Ernst, Kati J. and Vogelzang, Jayne and Gatesman, Taylor A. and Halbert, Matthew E. and Palova, Hana and Pokorna, Petra and Sterba, Jaroslav and Slaby, Ondrej and Geyeregger, Rene and Diaz, Aaron and Findlay, Izac J. and Dun, Matthew D. and Resnick, Adam and Suvà, Mario L. and Jones, David T. W. and Agnihotri, Sameer and Svedlund, Jessica and Koschmann, Carl and Haberler, Christine and Czech, Thomas and Slavc, Irene and Cotter, Jennifer A. and Ligon, Keith L. and Alexandrescu, Sanda and Yung, W. K. Alfred and Arrillaga-Romany, Isabel and Gojo, Johannes and Monje, Michelle and Nilsson, Mats and Filbin, Mariella G.},
  date = {2022-12-05},
  journaltitle = {Nat Genet},
  pages = {1--14},
  publisher = {Nature Publishing Group},
  issn = {1546-1718},
  doi = {10.1038/s41588-022-01236-3},
  url = {https://www.nature.com/articles/s41588-022-01236-3},
  urldate = {2022-12-05},
  abstract = {Histone 3 lysine27-to-methionine (H3-K27M) mutations most frequently occur in diffuse midline gliomas (DMGs) of the childhood pons but are also increasingly recognized in adults. Their potential heterogeneity at different ages and midline locations is vastly understudied. Here, through dissecting the single-cell transcriptomic, epigenomic and spatial architectures of a comprehensive cohort of patient H3-K27M DMGs, we delineate how age and anatomical location shape glioma cell-intrinsic and -extrinsic features in light of the shared driver mutation. We show that stem-like oligodendroglial precursor-like cells, present across all clinico-anatomical groups, display varying levels of maturation dependent on location. We reveal a previously underappreciated relationship between mesenchymal cancer cell states and age, linked to age-dependent differences in the immune microenvironment. Further, we resolve the spatial organization of H3-K27M DMG cell populations and identify a mitotic oligodendroglial-lineage niche. Collectively, our study provides a powerful framework for rational modeling and therapeutic interventions.},
  langid = {english},
  keywords = {Ageing,Cancer microenvironment,CNS cancer,Transcriptomics},
  file = {C\:\\Users\\jandrews\\Zotero\\storage\\TF62VKTM\\Liu et al. - 2022 - The landscape of tumor cell states and spatial org.pdf;C\:\\Users\\jandrews\\Zotero\\storage\\FY64KBLL\\s41588-022-01236-3.html}
}

