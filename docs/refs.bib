@article{sonabendTranscriptionalRegulatoryNetwork2014,
  title = {The {{Transcriptional Regulatory Network}} of {{Proneural Glioma Determines}} the {{Genetic Alterations Selected}} during {{Tumor Progression}}},
  author = {Sonabend, Adam M. and Bansal, Mukesh and Guarnieri, Paolo and Lei, Liang and Amendolara, Benjamin and Soderquist, Craig and Leung, Richard and Yun, Jonathan and Kennedy, Benjamin and Sisti, Julia and Bruce, Samuel and Bruce, Rachel and Shakya, Reena and Ludwig, Thomas and Rosenfeld, Steven and Sims, Peter A. and Bruce, Jeffrey N. and Califano, Andrea and Canoll, Peter},
