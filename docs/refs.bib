@article{kanCPEB4CellSurvival2010,
  title = {{{CPEB4 Is}} a {{Cell Survival Protein Retained}} in the {{Nucleus}} upon {{Ischemia}} or {{Endoplasmic Reticulum Calcium Depletion}}},
  author = {Kan, Ming-Chung and Oruganty-Das, Aparna and Cooper-Morgan, Amalene and Jin, Guang and Swanger, Sharon A. and Bassell, Gary J. and Florman, Harvey and van Leyen, Klaus and Richter, Joel D.},
