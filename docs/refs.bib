@article{schmidlinSpiBInhibitsHuman2008,
  title = {Spi-{{B}} Inhibits Human Plasma Cell Differentiation by Repressing {{BLIMP1}} and {{XBP-1}} Expression},
  author = {Schmidlin, Heike and Diehl, Sean A. and Nagasawa, Maho and Scheeren, Ferenc A. and Schotte, Remko and Uittenbogaart, Christel H. and Spits, Hergen and Blom, Bianca},
