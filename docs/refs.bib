@article{cresswellTADComparePackageDifferential2020,
  title = {{{TADCompare}}: {{An R Package}} for {{Differential}} and {{Temporal Analysis}} of {{Topologically Associated Domains}}},
  shorttitle = {{{TADCompare}}},
  author = {Cresswell, Kellen G. and Dozmorov, Mikhail G.},
  date = {2020},
  journaltitle = {Front. Genet.},
