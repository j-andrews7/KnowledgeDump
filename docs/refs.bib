@article{luETSTranscriptionFactor2023,
  title = {The {{ETS}} Transcription Factor {{ETV6}} Constrains the Transcriptional Activity of {{EWS}}–{{FLI}} to Promote {{Ewing}} Sarcoma},
  author = {Lu, Diana Y. and Ellegast, Jana M. and Ross, Kenneth N. and Malone, Clare F. and Lin, Shan and Mabe, Nathaniel W. and Dharia, Neekesh V. and Meyer, Ashleigh and Conway, Amy and Su, Angela H. and Selich-Anderson, Julia and Taslim, Cenny and Byrum, Andrea K. and Seong, Bo Kyung A. and Adane, Biniam and Gray, Nathanael S. and Rivera, Miguel N. and Lessnick, Stephen L. and Stegmaier, Kimberly},
  date = {2023-01-19},
  journaltitle = {Nat Cell Biol},
