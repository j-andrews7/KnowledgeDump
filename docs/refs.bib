@article{hakimDNADamageDefines2012,
  title = {{{DNA}} Damage Defines Sites of Recurrent Chromosomal Translocations in {{B}} Lymphocytes},
  author = {Hakim, Ofir and Resch, Wolfgang and Yamane, Arito and Klein, Isaac and Kieffer-Kwon, Kyong-Rim and Jankovic, Mila and Oliveira, Thiago and Bothmer, Anne and Voss, Ty C. and Ansarah-Sobrinho, Camilo and Mathe, Ewy and Liang, Genqing and Cobell, Jesse and Nakahashi, Hirotaka and Robbiani, Davide F. and Nussenzweig, Andre and Hager, Gordon L. and Nussenzweig, Michel C. and Casellas, Rafael},
