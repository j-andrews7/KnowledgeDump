  file = {C:\Users\jandrews\Zotero\storage\GDU2NVPV\Lin et al. - 2010 - The nuclear experience of CPEB Implications for R.pdf}
}

@article{linPhyloCSFComparativeGenomics2011,
  title = {{{PhyloCSF}}: A Comparative Genomics Method to Distinguish Protein Coding and Non-Coding Regions},
  shorttitle = {{{PhyloCSF}}},
  author = {Lin, Michael F. and Jungreis, Irwin and Kellis, Manolis},
  date = {2011-07-01},
