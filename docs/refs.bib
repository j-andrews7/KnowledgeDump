@article{ziffraSinglecellEpigenomicsReveals2021,
  title = {Single-Cell Epigenomics Reveals Mechanisms of Human Cortical Development},
  author = {Ziffra, Ryan S. and Kim, Chang N. and Ross, Jayden M. and Wilfert, Amy and Turner, Tychele N. and Haeussler, Maximilian and Casella, Alex M. and Przytycki, Pawel F. and Keough, Kathleen C. and Shin, David and Bogdanoff, Derek and Kreimer, Anat and Pollard, Katherine S. and Ament, Seth A. and Eichler, Evan E. and Ahituv, Nadav and Nowakowski, Tomasz J.},
  date = {2021-10},
  journaltitle = {Nature},
  volume = {598},
  number = {7879},
  pages = {205--213},
  publisher = {Nature Publishing Group},
  issn = {1476-4687},
  doi = {10.1038/s41586-021-03209-8},
  url = {https://www.nature.com/articles/s41586-021-03209-8},
  urldate = {2023-03-09},
  abstract = {During mammalian development, differences in chromatin state coincide with cellular differentiation and reflect changes in the gene regulatory landscape1. In the developing brain, cell fate specification and topographic identity are important for defining cell identity2 and confer selective vulnerabilities to neurodevelopmental disorders3. Here, to identify cell-type-specific chromatin accessibility patterns in the developing human brain, we used a single-cell assay for transposase accessibility by sequencing (scATAC-seq) in primary tissue samples from~the human forebrain. We applied unbiased analyses to identify genomic loci that undergo extensive cell-type- and brain-region-specific changes in accessibility during neurogenesis, and an integrative analysis to predict cell-type-specific candidate regulatory elements. We found that cerebral organoids recapitulate most putative cell-type-specific enhancer accessibility patterns but lack many cell-type-specific open chromatin regions that are found in vivo. Systematic comparison of chromatin accessibility across brain regions revealed unexpected diversity among neural progenitor cells in the cerebral cortex and implicated retinoic acid signalling in the specification of neuronal lineage identity in the prefrontal cortex. Together, our results reveal the important contribution of chromatin state to the emerging patterns of cell type diversity and cell fate specification and provide a blueprint for evaluating the fidelity and robustness of cerebral organoids as a model for cortical development.},
  issue = {7879},
  langid = {english},
  keywords = {Developmental neurogenesis,Neural patterning},
  file = {C:\Users\jandrews\Zotero\storage\2QUL4V42\Ziffra et al. - 2021 - Single-cell epigenomics reveals mechanisms of huma.pdf}
}

@article{zillerControllingComplementResistance2005,
  title = {Controlling Complement Resistance in Cancer by Using Human Monoclonal Antibodies That Neutralize Complement-Regulatory Proteins {{CD55}} and {{CD59}}},
  author = {Ziller, Federica and Macor, Paolo and Bulla, Roberta and Sblattero, Daniele and Marzari, Roberto and Tedesco, Francesco},
  date = {2005-07-01},
  journaltitle = {Eur. J. Immunol.},
  volume = {35},
  number = {7},
  pages = {2175--2183},
  issn = {1521-4141},
  doi = {10.1002/eji.200425920},
  url = {http://onlinelibrary.wiley.com/doi/10.1002/eji.200425920/abstract},
  urldate = {2015-08-31},
  abstract = {Expression of the complement-regulatory proteins (CRP) CD46, CD55 and CD59 represents a strategy used by tumor cells to evade complement-dependent cell cytoxicity stimulated by monoclonal antibodies. We have isolated two single-chain variable fragments (scFv) to CD55 and CD59 from a human phage-display library and from these scFv we have produced two miniantibodies (MB), MB-55 (against CD55) and MB-59 (against CD59), containing the human hinge–CH2–CH3 domains of IgG1. The specificity of the two MB for the corresponding CRP was assessed by ELISA using purified CD46, CD55 and CD59. MB-55 and MB-59 neutralized the inhibitory activity of CD55 and CD59, respectively, restoring the complement-mediated lysis of sheep and guinea pig erythrocytes that was otherwise inhibited by the two CRP. FACS analysis revealed binding of MB-55 and MB-59 to the lymphoma cell line Karpas 422. The two MB induced a two-fold increase in the complement-dependent killing of these cells stimulated by Rituximab, a chimeric anti-CD20 monoclonal antibody. Transfection of HEK293T cells with vectors encoding MB-55 or MB-59 markedly reduced the expression of CD55 and CD59. We conclude that the human antibodies MB-55 and MB-59 may represent a therapeutic tool to increase the complement-dependent killing activity of Rituximab in the treatment of non-Hodgkin's lymphoma.},
  langid = {english},
  keywords = {Complement,Complement regulatory proteins,Human monoclonal antibody,Lymphoma cell lines,Rituximab},
  file = {C\:\\Users\\jandrews\\Zotero\\storage\\S6UTRPC2\\Ziller et al. - 2005 - Controlling complement resistance in cancer by usi.pdf;C\:\\Users\\jandrews\\Zotero\\storage\\W93FSBAD\\full.html}
}

