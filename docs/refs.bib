}

@article{beguelinEZH2RequiredGerminal2013,
  title = {{{EZH2 Is Required}} for {{Germinal Center Formation}} and {{Somatic EZH2 Mutations Promote Lymphoid Transformation}}},
  author = {Béguelin, Wendy and Popovic, Relja and Teater, Matt and Jiang, Yanwen and Bunting, Karen~L. and Rosen, Monica and Shen, Hao and Yang, Shao~Ning and Wang, Ling and Ezponda, Teresa and Martinez-Garcia, Eva and Zhang, Haikuo and Zheng, Yupeng and Verma, Sharad~K. and McCabe, Michael~T. and Ott, Heidi~M. and Van~Aller, Glenn~S. and Kruger, Ryan~G. and Liu, Yan and McHugh, Charles~F. and Scott, David~W. and Chung, Young~Rock and Kelleher, Neil and Shaknovich, Rita and Creasy, Caretha~L. and Gascoyne, Randy~D. and Wong, Kwok-Kin and Cerchietti, Leandro and Levine, Ross~L. and Abdel-Wahab, Omar and Licht, Jonathan~D. and Elemento, Olivier and Melnick, Ari~M.},
