@article{hitoshiTosoCellSurface1998,
  title = {Toso, a {{Cell Surface}}, {{Specific Regulator}} of {{Fas-Induced Apoptosis}} in {{T Cells}}},
  author = {Hitoshi, Yasumichi and Lorens, James and Kitada, Shin-Ichi and Fisher, Joan and LaBarge, Mark and Ring, Huijun Z and Francke, Uta and Reed, John C and Kinoshita, Shigemi and Nolan, Garry P},
