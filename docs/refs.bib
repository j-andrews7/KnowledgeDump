@article{huangCooperativeTranscriptionalRepression2014,
  title = {Cooperative Transcriptional Repression by {{BCL6}} and {{BACH2}} in Germinal Center {{B-cell}} Differentiation},
  author = {Huang, Chuanxin and Geng, Huimin and Boss, Isaac and Wang, Ling and Melnick, Ari},
  date = {2014-02-13},
