@article{vaserSIFTMissensePredictions2016,
  title = {{{SIFT}} Missense Predictions for Genomes},
  author = {Vaser, Robert and Adusumalli, Swarnaseetha and Leng, Sim Ngak and Sikic, Mile and Ng, Pauline C.},
  date = {2016-01},
  journaltitle = {Nat. Protocols},
  volume = {11},
