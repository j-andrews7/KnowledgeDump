  author = {Dupuy, Adam J. and Rogers, Laura M. and Kim, Jinsil and Nannapaneni, Kishore and Starr, Timothy K. and Liu, Pentao and Largaespada, David A. and Scheetz, Todd E. and Jenkins, Nancy A. and Copeland, Neal G.},
  date = {2009-10-15},
  journaltitle = {Cancer Res},
  volume = {69},
