  author = {Chaber, Radosław and Gurgul, Artur and Wróbel, Grażyna and Haus, Olga and Tomoń, Anna and Kowalczyk, Jerzy and Szmatoła, Tomasz and Jasielczuk, Igor and Rybka, Blanka and Ryczan-Krawczyk, Renata and Duszeńko, Ewa and Stąpor, Sylwia and Ciebiera, Krzysztof and Paszek, Sylwia and Potocka, Natalia and Arthur, Christopher J. and Zawlik, Izabela},
  date = {2017-11-10},
  journaltitle = {PLOS ONE},
