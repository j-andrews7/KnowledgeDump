}

@article{drexlerFalseMycoplasmacontaminatedLeukemialymphoma2017,
  title = {False and Mycoplasma-Contaminated Leukemia-Lymphoma Cell Lines: Time for a Reappraisal},
  shorttitle = {False and Mycoplasma-Contaminated Leukemia-Lymphoma Cell Lines},
  author = {Drexler, Hans G. and Dirks, Wilhelm G. and MacLeod, Roderick A. F. and Uphoff, Cord C.},
  date = {2017-03-01},
  journaltitle = {Int J Cancer},
  volume = {140},
  number = {5},
  eprint = {27870004},
  eprinttype = {pmid},
  pages = {1209--1214},
  issn = {1097-0215},
  doi = {10.1002/ijc.30530},
  abstract = {Leukemia-lymphoma cell lines are important research tools in a variety of fields. To represent adequate model systems it is of utmost importance that cell lines faithfully model the primary tumor material and are not cross-contaminated with unrelated cell material (or contaminated with mycoplasma). As it has been previously reported that cross-contaminated cell lines represent a significant problem, it is of interest to know whether any improvement in the prevalence of such "false cell lines" had occurred since we called the alert in 1999. A retrospective review of our data archives covered 848 cell lines received from 1990 to 2014 from 290 laboratories in 23 countries spanning the spectrum of leukemia-lymphoma entities. Two variables were considered: authenticity and freedom from mycoplasma infection. Regarding provenance, we separately considered primary sources (original investigators having established the cell lines or reference repositories) and secondary sources. The percentages of mycoplasma-contaminated cell lines decreased significantly over the 25-year timespan. Among primary sourced material: mycoplasma-contamination fell from 23\% to 0\%; among secondary sourced: from 48\% to 21\%. The corresponding figures for cross-contamination declined from 15\% to 6\%, while among material obtained from secondary sources prevalence remained remarkably high, throughout the time periods at 14-18\%. Taken together, our data indicate that using non-authenticated cell lines from secondary sources carries a risk of about 1:6 for obtaining a false cell line. The use of authentic leukemia-lymphoma cell lines holds important translational value for their model character and the reproducibility of the laboratory data in the clinical arena.},
  langid = {english},
  keywords = {authentification,Cell Line Tumor,cell lines,DNA Fingerprinting,Humans,In Situ Hybridization,Karyotyping,leukemia,Leukemia,lymphoma,Lymphoma,mycoplasma,Mycoplasma,Retrospective Studies}
}

@article{ducAntigenBindingSecretory2010,
  title = {Antigen {{Binding}} to {{Secretory Immunoglobulin A Results}} in {{Decreased Sensitivity}} to {{Intestinal Proteases}} and {{Increased Binding}} to {{Cellular Fc Receptors}}},
  author = {Duc, Mélanie and Johansen, Finn-Eirik and Corthésy, Blaise},
  date = {2010-01-08},
  journaltitle = {J. Biol. Chem.},
  volume = {285},
  number = {2},
  eprint = {19910466},
  eprinttype = {pmid},
  pages = {953--960},
  issn = {0021-9258, 1083-351X},
  doi = {10.1074/jbc.M109.059220},
  url = {http://www.jbc.org/content/285/2/953},
  urldate = {2018-04-19},
  abstract = {In intestinal secretions, secretory IgA (SIgA) plays an important sentinel and protective role in the recognition and clearance of enteric pathogens. In addition to serving as a first line of defense, SIgA and SIgA·antigen immune complexes are selectively transported across Peyer's patches to underlying dendritic cells in the mucosa-associated lymphoid tissue, contributing to immune surveillance and immunomodulation. To explain the unexpected transport of immune complexes in face of the large excess of free SIgA in secretions, we postulated that SIgA experiences structural modifications upon antigen binding. To address this issue, we associated specific polymeric IgA and SIgA with antigens of various sizes and complexity (protein toxin, virus, bacterium). Compared with free antibody, we found modified sensitivity of the three antigens assayed after exposure to proteases from intestinal washes. Antigen binding further impacted on the immunoreactivity toward polyclonal antisera specific for the heavy and light chains of the antibody, as a function of the antigen size. These conformational changes promoted binding of the SIgA-based immune complex compared with the free antibody to cellular receptors (FcαRI and polymeric immunoglobulin receptor) expressed on the surface of premyelocytic and epithelial cell lines. These data reveal that antigen recognition by SIgA triggers structural changes that confer to the antibody enhanced receptor binding properties. This identifies immune complexes as particular structural entities integrating the presence of bound antigens and adds to the known function of immune exclusion and mucus anchoring by SIgA.},
  langid = {english},
  keywords = {Antibodies/Monoclonal,Cell/Epithelial,Immunology,Mucosal Immunology,Organisms/Bacteria,Receptors/Leukocyte/Lymphocyte,Toxins,Viruses/Immunology},
  file = {C\:\\Users\\jandrews\\Zotero\\storage\\ADBN6EZG\\Duc et al. - 2010 - Antigen Binding to Secretory Immunoglobulin A Resu.pdf;C\:\\Users\\jandrews\\Zotero\\storage\\CFVL3ZYI\\953.html}
}

@article{dumitruAHNAKInflammatoryMarkers2013,
  title = {{{AHNAK}} and Inflammatory Markers Predict Poor Survival in Laryngeal Carcinoma},
  author = {Dumitru, Claudia A. and Bankfalvi, Agnes and Gu, Xiang and Zeidler, Reinhard and Brandau, Sven and Lang, Stephan},
  date = {2013},
  journaltitle = {PLoS ONE},
  volume = {8},
  number = {2},
  eprint = {23409183},
  eprinttype = {pmid},
  pages = {e56420},
  issn = {1932-6203},
  doi = {10.1371/journal.pone.0056420},
  abstract = {AHNAK/Desmoyokin is a giant protein which has been recently linked to reorganization of the actin cytoskeleton, cellular migration and invasion. Here, we investigated the role of AHNAK in the pathophysiology of larynx carcinoma-one of the major subtypes of head and neck cancer. To this end, we analysed AHNAK expression in tumor tissues from 83 larynx carcinoma patients in relation to overall survival. We found that tumoral AHNAK overexpression significantly associated with poor survival of these patients both in univariate and multivariate analysis. In further studies, we combined the prognostic value of AHNAK with selected markers of inflammation, such as macrophage migration inhibitory factor (MIF) and tumor-infiltrating neutrophils (CD66b-positive cells). Both MIF and neutrophils have been linked to enhanced tumoral migration and poor clinical outcome in patients with orohypopharynx carcinoma-another major subtype of head and neck cancer. Interestingly, we found that synchronous high levels of AHNAK and MIF or AHNAK and neutrophils, respectively, were stronger predictors of poor survival than AHNAK alone. Synchronous high levels of all three markers were the strongest predictors of poor survival in our patient cohort. Taken together, our findings propose novel strategies for an accurate prognosis in larynx carcinoma and suggest potential mechanisms of inflammation-mediated tumor progression.},
  langid = {english},
  pmcid = {PMC3567070},
  keywords = {Antigens CD,Biomarkers Tumor,Cell Adhesion Molecules,Disease Progression,Female,Gene Expression Regulation Neoplastic,GPI-Linked Proteins,Humans,Inflammation,Laryngeal Neoplasms,Macrophage Migration-Inhibitory Factors,Male,Membrane Proteins,Multivariate Analysis,Neoplasm Proteins,Prognosis,Survival Analysis},
  file = {C:\Users\jandrews\Zotero\storage\IWR4WD6W\Dumitru et al. - 2013 - AHNAK and inflammatory markers predict poor surviv.pdf}
}

@article{dunleavyEmergingBiologicalInsights2015,
