@article{mullighanGenomewideAnalysisGenetic2007,
  title = {Genome-Wide Analysis of Genetic Alterations in Acute Lymphoblastic Leukaemia},
  author = {Mullighan, Charles G. and Goorha, Salil and Radtke, Ina and Miller, Christopher B. and Coustan-Smith, Elaine and Dalton, James D. and Girtman, Kevin and Mathew, Susan and Ma, Jing and Pounds, Stanley B. and Su, Xiaoping and Pui, Ching-Hon and Relling, Mary V. and Evans, William E. and Shurtleff, Sheila A. and Downing, James R.},
