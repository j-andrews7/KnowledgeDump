@article{seumoisEpigenomicAnalysisPrimary2014,
  title = {Epigenomic Analysis of Primary Human {{T}} Cells Reveals Enhancers Associated with {{TH2}} Memory Cell Differentiation and Asthma Susceptibility},
  author = {Seumois, Grégory and Chavez, Lukas and Gerasimova, Anna and Lienhard, Matthias and Omran, Nada and Kalinke, Lukas and Vedanayagam, Maria and Ganesan, Asha Purnima V. and Chawla, Ashu and Djukanović, Ratko and Ansel, K. Mark and Peters, Bjoern and Rao, Anjana and Vijayanand, Pandurangan},
