@article{chaligneEpigeneticEncodingHeritability2021,
  title = {Epigenetic Encoding, Heritability and Plasticity of Glioma Transcriptional Cell States},
  author = {Chaligne, Ronan and Gaiti, Federico and Silverbush, Dana and Schiffman, Joshua S. and Weisman, Hannah R. and Kluegel, Lloyd and Gritsch, Simon and Deochand, Sunil D. and Gonzalez Castro, L. Nicolas and Richman, Alyssa R. and Klughammer, Johanna and Biancalani, Tommaso and Muus, Christoph and Sheridan, Caroline and Alonso, Alicia and Izzo, Franco and Park, Jane and Rozenblatt-Rosen, Orit and Regev, Aviv and Suvà, Mario L. and Landau, Dan A.},
