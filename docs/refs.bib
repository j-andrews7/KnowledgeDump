@article{karrethBRAFPseudogeneFunctions2015,
  title = {The {{BRAF Pseudogene Functions}} as a {{Competitive Endogenous RNA}} and {{Induces Lymphoma In~Vivo}}},
  author = {Karreth, Florian A. and Reschke, Markus and Ruocco, Anna and Ng, Christopher and Chapuy, Bjoern and Léopold, Valentine and Sjoberg, Marcela and Keane, Thomas M. and Verma, Akanksha and Ala, Ugo and Tay, Yvonne and Wu, David and Seitzer, Nina and Velasco-Herrera, Martin Del Castillo and Bothmer, Anne and Fung, Jacqueline and Langellotto, Fernanda and Rodig, Scott J. and Elemento, Olivier and Shipp, Margaret A. and Adams, David J. and Chiarle, Roberto and Pandolfi, Pier Paolo},
