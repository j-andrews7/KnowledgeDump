}

@article{schmidtIncreasingGenomicEpigenomic2014,
  title = {Increasing Genomic and Epigenomic Complexity in the Clonal Evolution from {\emph{in Situ}} to Manifest t(14;18)-Positive Follicular Lymphoma},
  author = {Schmidt, J. and Salaverria, I. and Haake, A. and Bonzheim, I. and Adam, P. and Montes-Moreno, S. and Piris, M. A. and Fend, F. and Siebert, R. and Quintanilla-Martinez, L.},
  date = {2014-05},
  journaltitle = {Leukemia},
  volume = {28},
