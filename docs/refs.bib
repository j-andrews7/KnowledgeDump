@article{syrettLossXistRNA2017,
  title = {Loss of {{Xist RNA}} from the Inactive {{X}} during {{B}} Cell Development Is Restored in a Dynamic {{YY1-dependent}} Two-Step Process in Activated {{B}} Cells},
  author = {Syrett, Camille M. and Sindhava, Vishal and Hodawadekar, Suchita and Myles, Arpita and Liang, Guanxiang and Zhang, Yue and Nandi, Satabdi and Cancro, Michael and Atchison, Michael and Anguera, Montserrat C.},
  date = {2017-10-09},
  journaltitle = {PLOS Genetics},
  volume = {13},
  number = {10},
  pages = {e1007050},
  issn = {1553-7404},
  doi = {10.1371/journal.pgen.1007050},
  url = {http://journals.plos.org/plosgenetics/article?id=10.1371/journal.pgen.1007050},
  urldate = {2018-07-29},
  abstract = {X-chromosome inactivation (XCI) in female lymphocytes is uniquely regulated, as the inactive X (Xi) chromosome lacks localized Xist RNA and heterochromatin modifications. Epigenetic profiling reveals that Xist RNA is lost from the Xi at the pro-B cell stage and that additional heterochromatic modifications are gradually lost during B cell development. Activation of mature B cells restores Xist RNA and heterochromatin to the Xi in a dynamic two-step process that differs in timing and pattern, depending on the method of B cell stimulation. Finally, we find that DNA binding domain of YY1 is necessary for XCI in activated B cells, as ex-vivo YY1 deletion results in loss of Xi heterochromatin marks and up-regulation of X-linked genes. Ectopic expression of the YY1 zinc finger domain is sufficient to restore Xist RNA localization during B cell activation. Together, our results indicate that Xist RNA localization is critical for maintaining XCI in female lymphocytes, and that chromatin changes on the Xi during B cell development and the dynamic nature of YY1-dependent XCI maintenance in mature B cells predisposes X-linked immunity genes to reactivation.},
  langid = {english},
  keywords = {B cells,Bone marrow cells,Gene expression,Heterochromatin,Lymphocytes,RNA isolation,RNA-binding proteins,X-linked traits},
  file = {C\:\\Users\\jandrews\\Zotero\\storage\\7WMZMZ3Q\\Syrett et al. - 2017 - Loss of Xist RNA from the inactive X during B cell.pdf;C\:\\Users\\jandrews\\Zotero\\storage\\SLJ9K9P5\\article.html}
}

