@article{luMiR155PUAxis2014,
  title = {The {{miR-155}}–{{PU}}.1 Axis Acts on {{Pax5}} to Enable Efficient Terminal {{B}} Cell Differentiation},
  author = {Lu, Dong and Nakagawa, Rinako and Lazzaro, Sandra and Staudacher, Philipp and Abreu-Goodger, Cei and Henley, Tom and Boiani, Sara and Leyland, Rebecca and Galloway, Alison and Andrews, Simon and Butcher, Geoffrey and Nutt, Stephen L. and Turner, Martin and Vigorito, Elena},
  date = {2014-10-20},
  journaltitle = {Journal of Experimental Medicine},
