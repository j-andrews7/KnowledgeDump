  author = {Fei, Teng and Xia, Kai and Li, Zhongwei and Zhou, Bing and Zhu, Shanshan and Chen, Hua and Zhang, Jianping and Chen, Zhang and Xiao, Huasheng and Han, Jing-Dong J. and Chen, Ye-Guang},
  date = {2010-01-01},
