@article{altMechanismsProgrammedDNA2013,
  title = {Mechanisms of {{Programmed DNA Lesions}} and {{Genomic Instability}} in the {{Immune System}}},
  author = {Alt, Frederick~W. and Zhang, Yu and Meng, Fei-Long and Guo, Chunguang and Schwer, Bjoern},
