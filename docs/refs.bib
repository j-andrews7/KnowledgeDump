  author = {McLaughlin, P. and Grillo-López, A. J. and Link, B. K. and Levy, R. and Czuczman, M. S. and Williams, M. E. and Heyman, M. R. and Bence-Bruckler, I. and White, C. A. and Cabanillas, F. and Jain, V. and Ho, A. D. and Lister, J. and Wey, K. and Shen, D. and Dallaire, B. K.},
