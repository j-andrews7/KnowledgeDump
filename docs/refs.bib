@article{mollerConcanavalinAinducedBcell1986,
  title = {Concanavalin {{A-induced B-cell}} Proliferation Mediated by Allogeneically Derived Helper Factors.},
  author = {Möller, S A and Danielsson, L and Borrebaeck, C A},
  date = {1986-03},
