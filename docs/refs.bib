  author = {Lin, Shan and Larrue, Clément and Scheidegger, Nastassja K. and Seong, Bo Kyung A. and Dharia, Neekesh V. and Kuljanin, Miljan and Wechsler, Caroline S. and Kugener, Guillaume and Robichaud, Amanda L. and Conway, Amy Saur and Mashaka, Thelma and Mouche, Sarah and Adane, Biniam and Ryan, Jeremy A. and Mancias, Joseph D. and Younger, Scott T. and Piccioni, Federica and Lee, Lynn H. and Wunderlich, Mark and Letai, Anthony and Tamburini, Jérôme and Stegmaier, Kimberly},
  date = {2022-02-01},
  journaltitle = {Cancer Discovery},
