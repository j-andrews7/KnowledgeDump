  pmcid = {PMC3072784},
  keywords = {Amino Acid Sequence,Animals,Endosomes,Humans,Immunoglobulin A,Liver,Mice,Molecular Sequence Data,Protein Kinases,Proto-Oncogene Proteins c-yes,rab GTP-Binding Proteins,Rats,Receptor Epidermal Growth Factor,Receptors Polymeric Immunoglobulin,Sequence Alignment,Signal Transduction,Transcytosis}
}

@article{sunEpigenomicProfilingYoung2014,
  title = {Epigenomic {{Profiling}} of {{Young}} and {{Aged HSCs Reveals Concerted Changes}} during {{Aging}} That {{Reinforce Self-Renewal}}},
  author = {Sun, Deqiang and Luo, Min and Jeong, Mira and Rodriguez, Benjamin and Xia, Zheng and Hannah, Rebecca and Wang, Hui and Le, Thuc and Faull, Kym~F. and Chen, Rui and Gu, Hongcang and Bock, Christoph and Meissner, Alexander and Göttgens, Berthold and Darlington, Gretchen~J. and Li, Wei and Goodell, Margaret~A.},
